{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.source.neco.shortcut": "VIM",
  "coc.source.omni.filetypes": ["foam"],
  "coc.source.ultisnips.enable": false,
  "codeLens.enable": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.hintSign": "➤",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.warningSign": "!",
  "list.height": 20,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": true,
  "pyright.disableDiagnostics": true,
  "rust-analyzer.serverPath": "rust-analyzer",
  "suggest.enablePreview": true,
  "suggest.minTriggerInputLength": 3,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": "[+] ",
  "suggest.timeout": 1000
}
