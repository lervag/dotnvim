{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.source.neco.shortcut": "",
  "codeLens.enable": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.virtualText": true,
  "diagnostic.warningSign": "",
  "list.height": 20,
  "suggest.completionItemKindLabels": {
    "keyword":       "",
    "variable":      "",
    "value":         "",
    "operator":      "Ψ",
    "constructor":   "",
    "function":      "ƒ",
    "reference":     "渚",
    "constant":      "",
    "method":        "",
    "struct":        "פּ",
    "class":         "",
    "interface":     "",
    "text":          "",
    "enum":          "",
    "enumMember":    "",
    "module":        "",
    "color":         "",
    "property":      "",
    "field":         "料",
    "unit":          "",
    "event":         "鬒",
    "file":          "",
    "folder":        "",
    "snippet":       "",
    "typeParameter": "",
    "default":       ""
  },
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.enablePreview": true,
  "suggest.minTriggerInputLength": 3,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": " ﲖ ",
  "suggest.timeout": 1000,
  "snippets.extends": {"latex": ["tex"]},
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": true,
  "pyright.disableDiagnostics": true,
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url":"https://json.schemastore.org/package.json"
    }
  ],
  "ltex.checkFrequency": "save",
  "ltex.ltex-ls.logLevel": "config",
  "sumneko-lua.enableNvimLuaDev": true
}
