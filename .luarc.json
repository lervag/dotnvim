{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library",
      "$HOME/.local/plugged/mini.icons",
      "$HOME/.local/plugged/lazy.nvim",
      "$HOME/.local/plugged/snacks.nvim",
      "$HOME/.local/plugged/blink.cmp"
    ],
    "checkThirdParty": false
  }
}
