{
  "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua",
      "?/lua/?.lua",
      "?/lua/?/init.lua"
    ]
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "$HOME/.local/plugged/mini.icons",
      "$HOME/.local/plugged/mini.notify",
      "$HOME/.local/plugged/mini.snippets",
      "$HOME/.local/plugged/mini.pick",
      "$HOME/.local/plugged/mini.extra",
      "$HOME/.local/plugged/lazy.nvim",
      "$HOME/.local/plugged/snacks.nvim"
    ]
  },
  "diagnostics": {
    "globalsRegex": ["^Mini"]
  }
}
